{
    "name": "gitvault",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker:2": {
            "moby": false
        }
    },
    "customizations": {
        "vscode": {
            "settings": {
                "gopls": {
                    "ui.semanticTokens": true
                }
            },
            "extensions": [
                "golang.go"
            ]
        }
    },
    "remoteUser": "vscode"
}